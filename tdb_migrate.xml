<?xml version="1.0" ?>
<!DOCTYPE jupgrade>
<jupgrade>
	<name>Tweet Display Back</name>
	<!-- Adapter class location and name -->
	<installer>
		<file>modules/mod_tweetdisplayback/tdb_migrate.php</file>
		<class>jUpgradeModuleTDB</class>
	</installer>
	<!-- Updateservers for Joomla! Core Updater -->
	<updateservers>
		<server type="extension" name="Tweet Display Back Updates" priority="2">http://www.flbab.com/updates/TDB.xml</server>
	</updateservers>
	<!-- The folders to copy to the new site. -->
	<folders>
		<folder>modules/mod_tweetdisplayback/</folder>
	</folders>
</jupgrade>
